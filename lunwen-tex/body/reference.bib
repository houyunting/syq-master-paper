@inproceedings{2002287011810 ,
language = {English},
copyright = {Compilation and indexing terms, Copyright 2011 Elsevier Inc.},
copyright = {Compendex},
title = {A novel cost-effective multi-path adaptive interpolated FIR (IFIR)-based echo canceller},
journal = {Proceedings - IEEE International Symposium on Circuits and Systems},
author = {Wu, Cheng-Shing and Wu, An-Yeu Andy},
volume = {5},
year = {2002},
pages = {V/453 - V/456},
issn = {02714310},
address = {Phoenix, AZ, United states},
abstract = {In this paper, a multi-path adaptive interpolated FIR (AIFIR)-based echo canceller is presented to perform the echo cancellation in full-duplex digital transmission over DSL. The proposed multipath approach inherits the concept of AIFIR-based echo canceller, where the long tail portion is modelled by an adaptive sparse FIR filter. A multi-path structure is addressed to break the inherent design tradeoffs in the conventional AIFIR-based echo canceller. More than one adaptive IFIR filters, modelling the their corresponding tail portions of echo path, are employed. Also, an efficient implementation of the Image Compress Filter (ICF) is provided to reduce the complexity in performing the function of multi-ICFs. In addition, we apply the proposed scheme to the design of echo canceller in SHDSL transceiver. Computer simulations show that the flexible multi-path AIFIR-based echo canceller can reduce 20% computational complexity compared with previous works. About 60% complexity saving is obtained compared with the direct transversal implementation of echo canceller.},
key = {Echo suppression},
keywords = {Computational complexity;Computer simulation;Digital communication systems;FIR filters;Transceivers;},
note = {Echo canceller;Image compress filter;},
URL = {http://dx.doi.org/10.1109/ISCAS.2002.1010738},
} 

